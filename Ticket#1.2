#include <iostream>
#include <vector>
using namespace std;

int Digitsum(int n) {
	int sum = 0;
	while (n > 0) {
		sum += n % 10;
		n = n / 10;
	}
	return sum;
}
const int NumOfNum = 5; //Кол-во вводимых чисел
int main() {
	vector <int> Numbers(NumOfNum);
	int count = 0;
	int max_number = 0;
	//Ввод чисел в массив
	for (int i = 0; i < NumOfNum; i++) {
		cin >> Numbers[i];
		if (Digitsum(Numbers[i]) % 3 == 0) {
			count += 1;
			if (Numbers[i] > max_number)
				max_number = Numbers[i];
		}
	}
	// Вывод информации
	cout << "Number of numbers, which digit sum muitiple of three is " << count << endl;
	cout << "Maximum number, which digit sum muitiple of three is " << max_number;
	return 0;
}

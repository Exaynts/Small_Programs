#include <iostream>
#include <math.h>
#include <vector>
#include <string>
using namespace std;

bool IsPrime(int p) {
	for (int a = 2; a <= pow(p, 0.5); a++) {
		if (p % a == 0)
			return false;
	}
	if (p < 2)
		return false;
	return true;
}
const int n = 10; // Параметр перебора чисел
// Всего будет n^2 чисел
int main() {
	// Cоздание массивов
	vector<vector<string>> Num(n, vector <string>(n));
	vector <int> Primes;
	int lengthPrimes = 0;
	// Заполнение массива числами и для простых словом "simple"
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			int p = 10 * i + j;
			if (IsPrime(p) == true) {
				Num[i][j] = "simple";
				Primes.push_back(p);
				lengthPrimes += 1;
			}
			else
				Num[i][j] = to_string(p);
		}
	}
	// Вывод массивов
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cout << Num[i][j] << " ";
		}
		cout << endl;
	}
	cout << endl;
	for (int i = 0; i < lengthPrimes; i++) {
		cout << Primes[i] << " ";
	}
	return 0;
}
